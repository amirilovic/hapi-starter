{
  "name": "api",
  "version": "1.0.0",
  "description": "HapiJS Starter",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/npm-run-all compile up",
    "compile": "./node_modules/.bin/tsc",
    "up": "node dist/index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/lab -v -I '__core-js_shared__' dist/test/ && echo Test Passed ❤",
    "lint": "./node_modules/.bin/tslint 'src/**/*.ts' && echo Lint Passed ❤",
    "start-dev": "node_modules/.bin/npm-run-all --parallel compile-watch up-dev",
    "compile-watch": "./node_modules/.bin/tsc -w",
    "up-dev": "node_modules/.bin/nodemon --inspect=0.0.0.0:9229 dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "boom": "^7.1.1",
    "bunyan": "^1.8.12",
    "config": "^1.28.1",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "hapi": "^16.6.2",
    "hapi-swagger": "^8.0.0",
    "hoek": "^5.0.2",
    "inert": "^4.2.1",
    "joi": "^13.0.1",
    "rollbar": "^2.3.1",
    "vision": "^4.1.0"
  },
  "devDependencies": {
    "@types/boom": "^4.3.8",
    "@types/bunyan": "^1.8.3",
    "@types/config": "0.0.33",
    "@types/hapi": "^16.1.11",
    "@types/inert": "^4.2.4",
    "@types/joi": "^10.6.0",
    "@types/node": "^8.0.51",
    "@types/vision": "^4.1.3",
    "code": "^5.1.2",
    "lab": "^15.1.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
